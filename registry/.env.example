# NPM Registry Configuration
NPM_REGISTRY=https://registry.npmmirror.com

# Database Path
DB_PATH=./data/registry.db

# Batch Processing
BATCH_SIZE=10000                    # Index batch size (records per fetch)
ENRICH_BATCH_SIZE=100              # Enrichment batch size
ENRICH_CONCURRENCY=10              # Concurrent enrichment requests

# HTTP Configuration
REQUEST_TIMEOUT=60000              # Request timeout in milliseconds
REQUEST_DELAY=5                    # Delay between requests in milliseconds

# Output
OUTPUT_FILE=./data/packages.csv    # Default CSV output file

