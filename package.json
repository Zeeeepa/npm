{
  "name": "npm-registry-indexer",
  "version": "4.0.0",
  "description": "Modular NPM registry indexer with JSON export and database storage (PostgreSQL/MySQL)",
  "main": "index.js",
  "bin": {
    "npm-indexer": "./index.js"
  },
  "scripts": {
    "init": "node initialize.js",
    "sync": "node sync.js",
    "enrich": "node enrich.js",
    "export-csv": "node export-csv.js",
    "full-pipeline": "npm run init && npm run sync && npm run enrich && npm run export-csv",
    "index": "node index.js",
    "index:json": "node index.js --mode=json",
    "index:db": "node index.js --mode=db",
    "index:both": "node index.js --mode=both",
    "validate": "node tools/validate-output.js",
    "legacy:init": "node initialize_index.js",
    "legacy:sync": "node sync_index.js",
    "legacy:enrich": "node enrich_index.js"
  },
  "dependencies": {
    "co": "^4.6.0",
    "urllib": "^2.37.4",
    "pg": "^8.11.3",
    "mysql2": "^3.6.5",
    "csv-writer": "^1.6.0"
  },
  "devDependencies": {
    "normalize-registry-metadata": "^1.1.2",
    "semver": "^7.5.4",
    "humanize-ms": "^1.2.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "npm",
    "registry",
    "indexer",
    "postgres",
    "postgresql",
    "mysql",
    "database",
    "json",
    "streaming"
  ],
  "author": "Zeeeepa",
  "license": "MIT"
}
